{
  "version": 2,
  "builds": [
    {
      "src": "dist/main",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/*",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"],
      "rewrite": {
        "destination": "http://localhost:8000",
        "permanent": false
      }
    }
  ]
}
